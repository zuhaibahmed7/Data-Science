



                        #Frozenset()

fruits_set = {"banana", "Orange", "Apple"}
frozen_fruits = frozenset(fruits_set)           #---> can not add or remove the elements in this set cuz this is immutable
print(frozen_fruits)



                        #Set Operations
set1 = {1, 2, 3, 4, 5}
set2 = {4, 5, 6, 7, 8}

print(set1.difference(set2))        #Difference
print(set1 | set2)                   #Union
print(set1.union(set2))

print(set1.intersection(set2))       #Intersection(common in both sets)
print(set1 & set2)


print(set1.symmetric_difference(set2))          



